@model IEnumerable<CoreCnice.Domain.ESHOP_AD_ITEM>

<section class="slidertop slider-overlay wow bounceInRight" data-wow-duration="2s">
    <div class="container">
        <div class="owl-carousel owl-theme">
            @foreach (var category in Model.ToList())
            {
                <div class="item">
                    <a href="@category.AD_ITEM_URL">
                        <img src="~/UploadImages/Data/Banner/@category.AD_ITEM_FILENAME" class="images_bk" />
                    </a>
                </div>
            }
        </div>
    </div>
</section>